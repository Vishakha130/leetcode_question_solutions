class Solution {
    public boolean searchMatrix(int[][] matrix, int target) {
        int start=0;
        int m=matrix[0].length;
        int n=matrix.length;
        int end=n*m-1;
        while(start<=end)
        {
          int mid=start+(end-start)/2;
          int midel=matrix[mid/m][mid%m];
          if(target==midel)
          {
            return true;
          }
          else if(target<midel)
          {
            end=mid-1;
          }
          else
          {
            start=mid+1;
          }
        }
        return false;
    }
}
//Time Complexity:O(log(n*m))
//Space Complexity:O(1)
